{
  "entrypoint": "index.html",
  "fragments": [],
  "sources": [
    "src/**/*",
    "index.html"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js"
  ],
  "builds": [
    {
      "js": {
        "minify": true,
        "compile": true
      },
      "bundle": false,
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "insertPrefetchLinks": true,
      "addPushManifest": true,
      "addServiceWorker": true,
      "includeDependencies": [
        "bower_components/ace-builds/src-min-noconflict/ace.js",
        "bower_components/ace-builds/src-min-noconflict/ext-searchbox.js",
        "bower_components/ace-builds/src-min-noconflict/mode-html.js",
        "bower_components/ace-builds/src-min-noconflict/theme-monokai.js",
        "bower_components/ace-builds/src-min-noconflict/worker-html.js",
        "bower_components/wave-player/art.jpg"
      ]
    }
  ],
  "lint": {
    "rules": [
      "polymer-2-hybrid"
    ]
  }
}